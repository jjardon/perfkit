<?xml version="1.0" ?>
<node>
  <interface name="com.dronelabs.Perfkit.Channel">
    <annotation name="org.freedesktop.DBus.GLib.CSymbol" value="pkd_channel"/>

    <!-- Properties -->
    <property name="Dir" type="s" access="readwrite" />
    <property name="Args" type="as" access="readwrite" />
    <property name="Target" type="s" access="readwrite" />
    <property name="Env" type="as" access="readwrite" />
    <property name="Pid" type="u" access="readwrite" />

    <method name="GetTarget">
      <annotation name="org.freedesktop.DBus.GLib.CSymbol" value="pkd_channel_get_target_dbus"/>
      <arg name="target" type="s" direction="out"/>
    </method>
    <method name="GetArgs">
      <annotation name="org.freedesktop.DBus.GLib.CSymbol" value="pkd_channel_get_args_dbus"/>
      <arg name="args" type="as" direction="out"/>
    </method>

    <!-- Methods -->
    <method name="Start">
      <annotation name="org.freedesktop.DBus.GLib.CSymbol" value="pkd_channel_start"/>
    </method>
    <method name="Stop">
      <annotation name="org.freedesktop.DBus.GLib.CSymbol" value="pkd_channel_stop"/>
    </method>
    <method name="Pause">
      <annotation name="org.freedesktop.DBus.GLib.CSymbol" value="pkd_channel_pause"/>
    </method>
    <method name="Unpause">
      <annotation name="org.freedesktop.DBus.GLib.CSymbol" value="pkd_channel_unpause"/>
    </method>
  </interface>
</node>
